<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Semiproject</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  	<servlet>
		<servlet-name>BoardController</servlet-name>
		<servlet-class>com.semi.controller.BoardController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>BoardController</servlet-name>
		<url-pattern>/board.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>MemberController</servlet-name>
		<servlet-class>com.semi.controller.MemberController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>MemberController</servlet-name>
		<url-pattern>/member.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>StockController</servlet-name>
		<servlet-class>com.semi.controller.StockController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>StockController</servlet-name>
		<url-pattern>/stock.do</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>TradeController</servlet-name>
		<servlet-class>com.semi.controller.TradeController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>TradeController</servlet-name>
		<url-pattern>/trade.do</url-pattern>
	</servlet-mapping>

	<filter>
		<filter-name>Filter</filter-name>
		<filter-class>filter.Filter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>Filter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
  
</web-app>